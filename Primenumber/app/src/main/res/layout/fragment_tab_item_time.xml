<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:tools="http://schemas.android.com/tools"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             tools:context=".tabItem_time">

    <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        <Spinner
                android:layout_width="fill_parent"
                android:drawSelectorOnTop="true"
                android:layout_height="wrap_content" android:id="@+id/spinnerTime" tools:layout_marginTop="1000dp"/>
        <ListView
                android:layout_width="match_parent"
                android:layout_height="521dp" android:id="@+id/listTimeView" tools:layout_marginTop="300px"/>

        <Button
                android:text="Back To Main"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/button_time"
                tools:layout_marginBottom="150px"/>
    </LinearLayout>


</FrameLayout>